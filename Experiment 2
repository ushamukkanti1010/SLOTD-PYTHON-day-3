print("give a three digit number") 
a=int(input("enter a value"))
b=int(input("enter a value"))
c=int(input("enter a value"))
l=[]
l.append(a)
l.append(b)
l.append(c)
print(l)         
if (l[0] >= 10 or l[1]>= 10 or l[2]>= 10): 
       print("invalid,enter a valid number")
elif a<=0 or b<=0 or c<=0:
       print("Invalid")
else:
    for i in range (0,3):
     for j in range (0,3):
      for k in range (0,3):
        if (i!=j and j!=k and i!=k ):
          print(l[i],l[j],l[k])
